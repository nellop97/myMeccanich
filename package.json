{
  "name": "mymeccanich",
  "main": "index.js",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "// === SETUP DATABASE ===": "",
    "setup:firestore": "ts-node scripts/setupFirestore.ts",
    "setup:firestore:empty": "ts-node scripts/setupFirestore.ts empty",
    "setup:firestore:full": "ts-node scripts/setupFirestore.ts full",
    "setup:firestore:rules": "ts-node scripts/setupFirestore.ts rules",
    "setup:quick": "ts-node scripts/quickSetup.ts",
    "// === VALIDAZIONE ===": "",
    "db:validate": "ts-node scripts/validateData.ts",
    "db:check": "ts-node scripts/validateData.ts",
    "// === BACKUP/RESTORE ===": "",
    "db:backup": "ts-node scripts/backupFirestore.ts",
    "db:restore": "ts-node scripts/restoreFirestore.ts",
    "// === MANUTENZIONE ===": "",
    "db:migrate": "ts-node scripts/migrateData.ts",
    "db:clear": "ts-node scripts/clearDatabase.ts",
    "// === FIREBASE CLI ===": "",
    "firebase:init": "firebase init firestore",
    "firebase:deploy:rules": "firebase deploy --only firestore:rules",
    "firebase:deploy:indexes": "firebase deploy --only firestore:indexes",
    "firebase:deploy:all": "firebase deploy --only firestore",
    "// === SVILUPPO ===": "",
    "dev:setup": "npm run setup:quick && npm run db:validate",
    "dev:reset": "npm run db:clear -- --confirm && npm run setup:firestore:full",
    "dev:backup": "npm run db:backup && echo 'Backup salvato in scripts/backups/'",
    "// === PRODUZIONE ===": "",
    "prod:setup": "npm run setup:firestore:empty",
    "prod:validate": "npm run db:validate"
  },
