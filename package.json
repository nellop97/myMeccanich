{
  "name": "mymeccanich",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "// === DEVELOPMENT ===": "",
    "dev": "expo start --dev-client",
    "dev:android": "expo run:android --device",
    "dev:ios": "expo run:ios --device",
    "dev:web": "expo start --web",
    "// === BUILD ===": "",
    "build:preview": "eas build --profile preview",
    "build:production": "eas build --profile production",
    "prebuild": "expo prebuild --clean",
    "// === DATABASE SCRIPTS ===": "",
    "db:backup": "node scripts/backup.js",
    "db:restore": "node scripts/restore.js",
    "db:export": "node scripts/exportData.js",
    "db:import": "node scripts/importData.js",
    "db:validate": "node scripts/validateData.js",
    "db:clear": "node scripts/clearDatabase.js",
    "// === SETUP SCRIPTS ===": "",
    "setup:firestore:empty": "node scripts/setupFirestore.js",
    "setup:firestore:sample": "node scripts/setupFirestore.js --with-sample-data",
    "setup:firestore:full": "node scripts/setupFirestore.js --with-full-data",
    "// === UTILITY SCRIPTS ===": "",
    "dev:reset": "npm run db:clear -- --confirm && npm run setup:firestore:full",
    "dev:backup": "npm run db:backup && echo 'Backup salvato in scripts/backups/'",
    "// === PRODUZIONE ===": "",
    "prod:setup": "npm run setup:firestore:empty",
    "prod:validate": "npm run db:validate",
    "// === FIX & CLEANUP ===": "",
    "clean": "rm -rf node_modules package-lock.json && npm install",
    "clean:cache": "npx expo start -c",
    "fix:ios": "cd ios && pod deintegrate && pod install && cd ..",
    "fix:android": "cd android && ./gradlew clean && cd .."
  },
  "dependencies": {
