{
  "expo": {
    "name": "MyMechanic",
    "slug": "myMechanic",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "mymeccanich",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mymeccanich.com",
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true,
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "Questa app ha bisogno dell'accesso alla fotocamera per scattare foto dei veicoli e dei documenti.",
        "NSPhotoLibraryUsageDescription": "Questa app ha bisogno dell'accesso alla galleria foto per selezionare immagini dei veicoli e dei documenti."
      }
    },
    "android": {
      "package": "com.mymeccanich",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-dev-client",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          },
          "android": {
            "minSdkVersion": 21
          }
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "L'app necessita l'accesso alle tue foto per permetterti di aggiungere immagini dei veicoli.",
          "cameraPermission": "L'app necessita l'accesso alla fotocamera per scattare foto dei veicoli."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": false
    },
    "extra": {
      "eas": {
        "projectId": "ed142135-d0fd-4eb0-9424-99bc2efce5ea"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/ed142135-d0fd-4eb0-9424-99bc2efce5ea"
    }
  }
}