{
  "expo": {
    "name": "MyMechanic",
    "slug": "myMechanic",
    "version": "1.0.0",
    "orientation": "portrait",
    // "icon": "./logo.png", // Commenta questa riga
    "scheme": "mymeccanich",
    "userInterfaceStyle": "automatic",
    // "splash": { // Commenta l'intero blocco splash
    //   "image": "./splashscreen.png",
    //   "resizeMode": "contain",
    //   "backgroundColor": "#ffffff"
    // },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mymeccanich.com",
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true
      }
    },
    "android": {
      // "adaptiveIcon": { // Commenta l'intero blocco adaptiveIcon
      //   "foregroundImage": "./logo.png",
      //   "backgroundColor": "#ffffff"
      // },
      "package": "com.mymecchanich"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
      // "favicon": "./logo.png" /
    },
    "plugins": [
      "expo-dev-client",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": false
    }
  }
}
